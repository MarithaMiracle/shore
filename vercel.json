{
    "routes": [{
            "src": "/static/(.*)",
            "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
        },
        {
            "src": "/assets/(.*)",
            "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
        },
        {
            "src": "/favicon.ico",
            "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
        },
        {
            "src": "/(.*\\.[^/]+)$",
            "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/404.html",
            "status": 404
        }
    ]
}